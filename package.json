{
    "name": "jtsx-loader",
    "version": "0.1.10",
    "main": "./loader/register.mjs",
    "type": "module",
    "imports": {
        "#@/*": "./*"
    },
    "scripts": {
        "start": "node --import ./loader/register.mjs ./example/server.js",
        "dev": "nodemon --import ./loader/register.mjs ./example/server.js",
        "patch": "npm version patch && npm publish"
    },
    "engines": {
        "node": ">=20.16 <25"
    },
    "keywords": [
        "jsx",
        "tsx",
        "loader",
        "html",
        "static",
        "node",
        "template engine"
    ],
    "author": "Dergachev Mihail <dergachev.mihail@gmail.com>",
    "license": "MIT",
    "description": "Loader for node.js that allows you to load JSX and TSX files and use it as functions",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/dergachevm/jtsx-loader.git"
    },
    "dependencies": {
        "esbuild": "^0.27.1"
    },
    "devDependencies": {
        "express": "^5.2.1",
        "htmlfy": "^1.0.1",
        "nodemon": "^3.1.11"
    }
}
